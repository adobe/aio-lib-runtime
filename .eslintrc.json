{
  "extends": ["@adobe/eslint-config-aio-lib-config"],
  "settings": {
    "jsdoc": {
      "ignorePrivate": true
    }
  },
  "rules": {
    "no-param-reassign": [ "warn", { "props": true }],
    "jsdoc/tag-lines": [
      // The Error level should be `error`, `warn`, or `off` (or 2, 1, or 0)
      "error",
      "never",
      {
        "startLines": null
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}